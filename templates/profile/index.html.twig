{% extends 'base.html.twig' %}

{% block title %}Profile | Drop obServer{% endblock %}

{% block body %}
<div class="jumbotron jumbotron-fluid">
    <div class="container">
        <h1 class="display-4">Welcome to your profile</h1>
        <hr class="my-4">
        <form>
            <div class="form-group">
                <label for="twitterConsumerKey">twitterConsumerKey</label>
                <input type="text" class="form-control" id="twitterConsumerKey" placeholder="Enter twitterConsumerKey">
            </div>
            <div class="form-group">
                <label for="twitterConsumerSecret">twitterConsumerSecret</label>
                <input type="password" class="form-control" id="twitterConsumerSecret" aria-describedby="twitterConsumerSecretHelp" placeholder="Enter twitterConsumerSecret">
                <small id="twitterConsumerSecretHelp" class="form-text text-muted">We'll never share your twitterConsumerSecret with anyone else.</small>
            </div>
            <div class="form-group">
                <label for="twitterAccessToken">twitterAccessToken</label>
                <input type="text" class="form-control" id="twitterAccessToken" placeholder="Enter twitterAccessToken">
            </div>
            <div class="form-group">
                <label for="twitterAccessTokenSecret">twitterAccessTokenSecret</label>
                <input type="password" class="form-control" id="twitterAccessTokenSecret" aria-describedby="twitterAccessTokenSecretHelp" placeholder="Enter twitterAccessTokenSecret">
                <small id="twitterAccessTokenSecretHelp" class="form-text text-muted">We'll never share your twitterAccessTokenSecret with anyone else.</small>
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>
</div>
{% endblock %}
